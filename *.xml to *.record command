# Create train data:
python generate_tfrecord.py -x C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\images\train -l C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\annotations\label_map.pbtxt -o C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\annotations\train.record

# Create test data:
python generate_tfrecord.py -x C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\images\test -l C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\annotations\label_map.pbtxt -o C:\Users\Admin\Documents\TensorFlow\workspace\training_demo\annotations\test.record

